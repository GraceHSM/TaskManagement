<section class="container-fluid">
<table>
  <thead>
    <tr>
      <th>UserName</th>
      <th>Email</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= user.username %></td>
      <td><%= user.email %></td>
      <td><%= link_to 'Edit', edit_user_path(user) %><%= link_to 'Delete', user_path(user), method: 'delete' %></td>
    </tr>
    <% end %>
  </tbody>
</table>
<%= link_to 'NewUser', new_user_path %>
</section>